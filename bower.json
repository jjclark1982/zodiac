{
  "name": "your-app",
  "version": "0.0.1",
  "main": "public/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "console-polyfill": "~0.1.0",
    "modernizr": "~2.6.2",
    "jquery": "~1.9.1",
    "lodash": "~1.3.1",
    "backbone": "~1.0.0",
    "normalize-css": "~2.1.2",
    "font-awesome": "~3.2.1",
    "bourbon": "~3.1.8",
    "neat": "~1.3.0",
    "typeahead.js": "~0.9.3"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "lodash": "*",
        "jquery": "~1.9.1"
      },
      "main": "backbone.js"
    },
    "modernizr": {
      "main": "modernizr.js"
    },
    "font-awesome": {
      "main": "css/font-awesome.css"
    },
    "bourbon": {
      "main": "app/assets/stylesheets/_bourbon.scss"
    },
    "neat": {
      "main": "app/assets/stylesheets/_neat.scss"
    },
    "jquery": {
      "main": "jquery.js"
    },
    "typeahead.js": {
        "main": "dist/typeahead.js",
        "dependencies": {
          "jquery": "~1.9.1"
      }
    }
  }
}
